#Esse é um código que pega qualquer strin dentro do print e,
#independente de estar em CapsLock ou não ele converte todas as 
#letras para letras miúdas e usa como base o que estava em
#maiúscula para separar por "_"
def convert_to_snake_case(pascal_or_camel_cased_string):

    snake_cased_char_list = [
        '_' + char.lower() if char.isupper()
        else char
        for char in pascal_or_camel_cased_string
    ]

    return ''.join(snake_cased_char_list).strip('_')

def main():
    print(convert_to_snake_case('IAmAPascalCasedString'))

    

main()